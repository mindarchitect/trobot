<Window x:Class="TRobot.ECU.UI.Views.DataView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TRobot.ECU.UI.Views"
        mc:Ignorable="d"
        Title="DataView" WindowStartupLocation="CenterOwner"
        SizeToContent="WidthAndHeight" >
    <StackPanel>
        <Button Style="{StaticResource Button}" Content="Button" HorizontalAlignment="Left" VerticalAlignment="Top" Command="{Binding Path=GetDataCommand}" />
    </StackPanel>
</Window>
